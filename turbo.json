{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": [
    "REACT_APP_COGNITO_USER_POOL_ID",
    "REACT_APP_COGNITO_USER_POOL_CLIENT_ID"
  ]
}
